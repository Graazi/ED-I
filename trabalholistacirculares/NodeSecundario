public class NodeSecundario {
    private String nome;
    private String definicao;

    public NodeSecundario(String nome, String definicao) {
        this.nome = nome;
        this.definicao = definicao;
    }

    public String getNome() {
        return nome;
    }

    public String getDefinicao() {
        return definicao;
    }
}
